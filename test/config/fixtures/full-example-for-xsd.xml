                <property-default tag="upnp:director" value="UNKNOWN"/>
                <property-default tag="upnp:genre" value="UNKNOWN"/>
                <property-default tag="upnp:language" value="en-US"/>
                <property-default tag="upnp:lastPlaybackPosition" value="0"/>
                <property-default tag="upnp:lastPlaybackTime" value="1900-01-01T00:00:00"/>
                <property-default tag="upnp:lyricsURI" value=""/>
                <property-default tag="upnp:originalTrackNumber" value="0"/>
