                <container-type-image>object.container.album.musicAlbum</container-type-image>
                <container-type-video>object.container.album.musicAlbum</container-type-video>
            </directory>
            <directory location="/mnt/Video" mode="inotify" recursive="yes" follow-symlinks="yes" hidden-files="yes"/>
        </autoscan>
        <magic-file>magic.file</magic-file>
        <virtual-directories>
            <key metadata="M_ALBUMARTIST" class="object.container.album.musicAlbum"/>
            <key metadata="M_UPNP_DATE" class="object.container.album.musicAlbum"/>
