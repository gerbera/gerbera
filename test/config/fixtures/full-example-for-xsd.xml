    <server debug-mode="content|xml">
        <logging rotate-file-size="1000000" rotate-file-count="5"/>
        <ui enabled="yes" show-tooltips="yes" poll-interval="2" poll-when-idle="no" show-numbering="yes" show-thumbnail="yes" show-video="no">
            <content-security-policy>
                font-src %HOSTS% https://fonts.gstatic.com/
                style-src %HOSTS% https://fonts.googleapis.com 'unsafe-inline'
                img-src *
                media-src *
                child-src 'none'
                default-src %HOSTS% 'unsafe-eval' 'unsafe-inline'
            </content-security-policy>
            <accounts enabled="no" session-timeout="30">
                <account user="gerbera" password="gerbera"/>
            </accounts>
            <items-per-page default="25">
                <option>10</option>
                <option>25</option>
                <option>50</option>
                <option>100</option>
            </items-per-page>
            <extension-mimetype default="application/octet-stream">
                <map from="html" to="text/html"/>
                <map from="js" to="application/javascript"/>
                <map from="json" to="application/json"/>
                <map from="css" to="text/css"/>
            </extension-mimetype>
            <source-docs-link>./dev/index.html</source-docs-link>
            <user-docs-link>./doc/</user-docs-link>
        </ui>
        <name>Gerbera</name>
        <udn>uuid:fcc3b17d-01ab-41d6-99cb-068598464a3d</udn>
        <home override="no">/tmp/.config/gerbera</home>
        <webroot>/usr/local/share/gerbera/web</webroot>
        <port>50600</port>
        <ip>192.168.10.7</ip>
        <interface>eth0</interface>
        <manufacturer>Gerbera Contributors</manufacturer>
        <manufacturerURL>https://gerbera.io/</manufacturerURL>
        <modelName>Gerbera</modelName>
        <modelDescription>Free UPnP AV MediaServer, GNU GPL</modelDescription>
        <modelNumber>42</modelNumber>
        <modelURL>https://gerbera.io</modelURL>
        <serialNumber>20400</serialNumber>
        <presentationURL append-to="none"/>
        <tmpdir>/tmp/</tmpdir>
        <pc-directory upnp-hide="no"/>
        <bookmark>gerbera.html</bookmark>
        <upnp-string-limit>-1</upnp-string-limit>
        <virtualURL>https://gerbera.io:50600</virtualURL>
        <externalURL>https://gerbera.io</externalURL>
        <upnp literal-host-redirection="no" multi-value="yes" search-result-separator=" - " search-filename="no" searchable-container-flag="no" caption-info-count="1">
            <search-item-result>
                <add-data tag="M_ARTIST"/>
                <add-data tag="M_TITLE"/>
            </search-item-result>
            <album-properties extend="true">
                <upnp-property upnp-tag="upnp:icon" meta-data="M_ALBUMARTURI"/>
            </album-properties>
            <artist-properties>
                <upnp-property upnp-tag="upnp:albumArtist" meta-data="M_ALBUMARTIST"/>
                <upnp-property upnp-tag="upnp:artist" meta-data="M_ALBUMARTIST"/>
                <upnp-property upnp-tag="upnp:genre" meta-data="M_GENRE"/>
            </artist-properties>
            <genre-properties>
                <upnp-property upnp-tag="upnp:genre" meta-data="M_GENRE"/>
            </genre-properties>
            <playlist-properties>
                <upnp-property upnp-tag="dc:date" meta-data="M_UPNP_DATE"/>
            </playlist-properties>
            <title-properties/>
            <resource-defaults>
                <property-default tag="@allowedUse" value="UNKNOWN"/>
                <property-default tag="@bitrate" value="0"/>
                <property-default tag="@bitsPerSample" value="0"/>
                <property-default tag="@colorDepth" value="1"/>
