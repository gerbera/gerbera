{
  "name": "gerbera-web",
  "version": "1.11.1",
  "repository": "https://github.com/gerbera/gerbera",
  "license": "GPLv2",
  "scripts": {
    "start": "node index.js",
    "test": "karma start karma.conf.js",
    "test:e2e": "concurrently --success first --kill-others \"npm start\" \"npm run e2e\"",
    "e2e": "SELENIUM_BROWSER=chrome mocha --timeout 15000 \"test/e2e/**.spec.js\"",
    "screenshot": "rimraf _build && mkdirp _build/doc && mocha --timeout 15000 \"test/e2e/screenshot.doc.js\" --folderPath=/_build/doc/",
    "doc:screenshot": "concurrently --success first --kill-others \"npm start\" \"npm run screenshot\"",
    "lint": "eslint -c .eslintrc ../web/js/*gerbera*.js"
  },
  "config": {
    "webserver": {
      "port": 3000,
      "host": "localhost"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.19.1",
    "babel-loader": "^8.2.5",
    "babel-register": "^6.26.0",
    "bootstrap": "^4.6.1",
    "braces": "^3.0.2",
    "chai": "^4.3.6",
    "chromedriver": "^105.0.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.22.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "express": "^4.18.1",
    "jasmine-core": "^4.4.0",
    "jimp": "^0.16.2",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.13.2",
    "js-cookie": "^3.0.1",
    "karma": "^6.4.1",
    "karma-babel-preprocessor": "^8.0.2",
    "karma-chrome-launcher": "^3.1.1",
    "karma-fixture": "^0.2.6",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-jasmine": "^5.1.0",
    "karma-json-fixtures-preprocessor": "^0.0.6",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "^0.0.33",
    "karma-webpack": "^5.0.0",
    "lodash": "^4.17.21",
    "mkdirp": "^0.5.5",
    "mocha": "^10.0.0",
    "popper.js": "^1.16.1",
    "rimraf": "^3.0.2",
    "selenium-webdriver": "^4.4.0",
    "webpack": "^5.74.0",
    "yargs": "^17.5.1"
  }
}
