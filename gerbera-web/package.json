{
  "name": "gerbera-web",
  "version": "2.5.1",
  "repository": "https://github.com/gerbera/gerbera",
  "license": "GPLv2",
  "scripts": {
    "start": "node index.js",
    "test": "karma start karma.conf.js",
    "test:debug": "karma start karma.conf.js --log-level debug",
    "test:e2e": "concurrently --success first --kill-others \"npm start\" \"npm run e2e\"",
    "e2e": "SELENIUM_BROWSER=chrome mocha --timeout 15000 \"test/e2e/**.spec.js\"",
    "screenshot": "rimraf _build && mkdirp _build/doc && SELENIUM_BROWSER=chrome RUNFILES_DIR=node_modules/@bazel/runfiles/ mocha --timeout 15000 \"test/e2e/screenshot.doc.js\" --folderPath=/_build/doc/ --scheme=light",
    "screenshot-dark": "rimraf _build && mkdirp _build/doc && SELENIUM_BROWSER=chrome RUNFILES_DIR=node_modules/@bazel/runfiles/ mocha --timeout 15000 \"test/e2e/screenshot.doc.js\" --folderPath=/_build/doc/ --scheme=dark",
    "doc:screenshot": "concurrently --success first --kill-others \"npm start\" \"npm run screenshot\"",
    "doc:screenshot-dark": "concurrently --success first --kill-others \"npm start\" \"npm run screenshot-dark\"",
    "lint": "eslint -c .eslintrc ../web/js/*gerbera*.js"
  },
  "config": {
    "webserver": {
      "port": 3000,
      "host": "localhost"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.26.9",
    "@babel/register": "^7.25.9",
    "@popperjs/core": "^2.11.8",
    "babel-loader": "^9.2.1",
    "bootstrap": "^4.6.2",
    "braces": "^3.0.3",
    "chai": "^4.5.0",
    "chromedriver": "134.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.6.0",
    "express": "^4.21.2",
    "jasmine-core": "^5.6.0",
    "jimp": "^0.22.12",
    "jquery": "^3.7.1",
    "jquery-ui": "^1.14.1",
    "js-cookie": "^3.0.5",
    "karma": "^6.4.4",
    "karma-babel-preprocessor": "^8.0.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-fixture": "^0.2.6",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-jasmine": "^5.1.0",
    "karma-json-fixtures-preprocessor": "^0.0.6",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "^0.0.33",
    "karma-webpack": "^5.0.1",
    "lodash": "^4.17.21",
    "mkdirp": "^0.5.6",
    "mocha": "^10.8.2",
    "rimraf": "^6.0.1",
    "selenium-webdriver": "^4.29.0",
    "webpack": "^5.98.0",
    "yargs": "^17.7.2"
  }
}
